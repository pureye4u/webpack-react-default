{
  "name": "webpack-react-template",
  "version": "0.1.0-alpha.1",
  "description": "React webpack template for UI development",
  "repository": {
    "type": "git",
    "url": "https://github.com/pureye4u/webpack-react-default.git"
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack-dev-server.config.js --progress --inline --colors",
